{
    "extends": [
      "plugin:import/errors",
      "plugin:react/recommended",
      "plugin:jsx-a11y/recommended",
      "prettier",
      "prettier/react",
      "plugin:@typescript-eslint/recommended"
    ],
    "rules": {
      "react/prop-types": 0,
      "jsx-a11y/label-has-for": 0
      // "no-console":0
    },
    "plugins": ["react", "import", "jsx-a11y"],
    "parser": "@typescript-eslint/parse",
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "env": {
      "es6": true,
      "browser": true,
      "node": true
    },
    "settings": {
      "react": {
        "version": "16.13.1"
      },
      "import/resolver": {
        "node": {
          "paths": ["src"]
        }
      }
    }
  }
  